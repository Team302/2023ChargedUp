<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>
<primitive id="RESET_POSITION_PATH_PLANNER"
	           pathname="BlueHP1"
			/>
<!--score preloaded cone -->
    <snippet file="ScoreCone.xml"/>
<!--drive to cube-->
<primitive id="DRIVE_PATH_PLANNER"
	           pathname="BlueHP1"
             arm="FLOOR_POSITION_ROTATE_AUTON"
             intake="INTAKE"
             time="2.5"/> 
<primitive id="DO_NOTHING"
               arm="FLOOR_POSITION_ROTATE"
               extender="FLOOR_EXTEND"
               intake="INTAKE"
               time="0.25"
               />
<!--drive to cube/scoring station-->
<primitive id="DRIVE_PATH_PLANNER"
	          pathname="BlueHP2"
            arm="FLOOR_POSITION_ROTATE"
            extender="FLOOR_EXTEND"
				    intake="INTAKE"
            time="5.0"/>
<!--Score High Cube-->
<primitive id="DO_NOTHING"
               time="15"/> 
<primitive id="DRIVE_PATH_PLANNER"
	           pathname="BlueHP3"
               time="2.35"/>
<!--drive to scoring station-->
<primitive id="DRIVE_PATH_PLANNER"
	           pathname="BlueHP4P"
               time="2.5"/>
<!--score cone-->
  <!--auto balance-->
    <primitive id="AUTO_BALANCE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE_OFF"
               time="10.0"
               />
</auton>