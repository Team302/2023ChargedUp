<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>
<primitive id="RESET_POSITION"
	   pathname="BlueHPOne1.wpilib.json"
               intake="HOLD"/>
    <!--score preloaded cone-->
    <snippet file="ScoreCone.xml"/>
    <!--drive to charing station-->
    <primitive id="DRIVE_PATH"
               pathname="BlueHPOne1.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="180"
               arm="STARTING_POSITION_ROTATE"
               intake="INTAKE"
               time="0.5"
               />
    <primitive id="DRIVE_PATH"
               pathname="BlueHPOne4.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="180"
               arm="STARTING_POSITION_ROTATE"
               intake="INTAKE"
               time="0.75"
               />
    <primitive id="DRIVE_PATH"
               pathname="BlueHPTwo1.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               intake="INTAKE"
               time="8.0"
               />
   <!--grab cube-->
    <primitive id="DO_NOTHING"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               intake="INTAKE"
               time="0.5"
               />
   <!--drive to scoring station-->
    <primitive id="DRIVE_PATH"
               pathname="BlueHPTwo2.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               intake="HOLD"
               time="8.0"
               />
   <!--score cube-->
    <primitive id="DO_NOTHING"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               intake="INTAKE_RELEASE"
               time="0.5"
               />
</auton>