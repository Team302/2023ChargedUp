               arm="CUBE_BACKROW_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="0.5"
               />
    <!--drive over charging station-->
    <primitive id="DRIVE_PATH"
               pathname="RedCOOPOne1CubeSlow.wpilib.json"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="15.0"
               />
