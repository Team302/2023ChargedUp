<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>
<primitive id="RESET_POSITION"
	   pathname="BlueWallOne1.wpilib.json"
       grabber="GRAB"/>
   <!--score preloaded cone-->
   <primitive id="DO_NOTHING"
               arm="CONE_BACKROW_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="GRAB"
               time="0.35"
               />
    <primitive id="DO_NOTHING"
               arm="CONE_BACKROW_ROTATE"
               extender="CONE_BACKROW_EXTEND"
               grabber="GRAB"
               time="0.6"
               />
    <primitive id="DO_NOTHING"
               arm="CONE_BACKROW_ROTATE"
               extender="CONE_BACKROW_EXTEND"
               grabber="OPEN"
               time="0.35"
               />
    <primitive id="DO_NOTHING"
               arm="CONE_BACKROW_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="0.6"
               />
    <!--drive to charing station-->
    <primitive id="DRIVE_PATH"
               pathname="BlueWallOne1.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="180"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="0.8"
               />
    <primitive id="DRIVE_PATH"
               pathname="BlueWallOne4.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="180"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="4.0"
               />
    <primitive id="DRIVE_PATH"
               pathname="BlueWallOne2.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="180"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="5.0"
               />
    <primitive id="DRIVE_PATH"
               pathname="BlueWallOne3.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="2.0"
               />
    <!--drive onto charging station-->
    <primitive id="DRIVE_PATH"
               pathname="BlueWallOne1Slow.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="3.0"
               />
    <!--auto balance-->
    <primitive id="AUTO_BALANCE"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               grabber="OPEN"
               time="10.0"
               />
</auton>